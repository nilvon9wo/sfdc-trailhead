	public static String get(Schema.SObjectType sObjectType) 
	{
 		if (prefixBySObjectTypeCache.containsKey(sObjectType)) { return prefixBySObjectTypeCache.get(sObjectType); }

		String keyPrefix =  sObjectType.getDescribe().getKeyPrefix();
		prefixBySObjectTypeCache.put (sObjectType, keyPrefix);
		return keyPrefix;
	}


